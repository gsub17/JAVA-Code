public class LoopDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int counter = 1;
		while(counter <=5) {
			System.out.println("God please help me to get into mtech");
			counter++;
		}
		
		System.out.println("    ");
		
		int counter2 = 5;
		while(counter2-- != 0) {
			System.out.println("God please help me");
		}

	}

}
