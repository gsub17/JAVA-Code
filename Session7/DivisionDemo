
public class DivisionDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	//	int a = 4/7;
		//float a = 4/7;
	//	float a = 4f/7;
		float a = 4f/7f;
		System.out.println(a);
		
		double d = 4/7;
		System.out.println(d);
		
		double y = 4.0/7;
		System.out.println(y);

	}

}
