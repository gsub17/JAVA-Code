public class DataTypeDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		byte b = (byte) 640;
		System.out.println(b);
		
		short c = 128;
		System.out.println(c);
		
		for(int i =0 ;i <=127;i++) {
			System.out.println(i);
		}
		
		for(byte r = 0; r<=127;r++) {
			System.out.println(r);
		}

	}

}
