
import java.util.*;

public class FunctionWithParms {
	
	public static void additionParams(int a , int b) {
		int c = a+b;
		System.out.println(c);
		
		
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		additionParams(a, b);
	}

}
